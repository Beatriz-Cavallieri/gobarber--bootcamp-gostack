]);\n\n  useEffect(() => {\n    registerField({\n      name: fieldName,\n      ref: inputRef.current,\n      path: \"value\",\n    });\n  }, [fieldName, registerField]);\n\n  return (\n    <Container isFocused={isFocused} isFilled={isFilled} hasError={!!error}>\n      {Icon && <Icon size={20} />}\n      <input\n        onFocus={handleInputFocus}\n        onBlur={handleInputBlur}\n        ref={inputRef}\n        {...rest}\n      />\n      {error && (\n        <Error title={error} >\n          <FiAlertCircle size={20} color=\"#c53030\" />\n        </Error>\n      )}\n    </Container>\n  );\n};\n\nexport default Input;\n","/home/bfcav/revisao-gostack/gobarber/web/src/components/Button/Input/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/pages/SignUp/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/pages/SignUp/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/utils/getValidationErrors.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/Tooltip/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/Tooltip/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/services/api.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/ToastContainer/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/ToastContainer/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/hooks/auth.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/hooks/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/hooks/toast.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/ToastContainer/Toast/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/ToastContainer/Toast/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/routes/index.tsx",[],{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","severity":1,"message":"105","line":27,"column":11,"nodeType":"106","messageId":"107","endLine":27,"endColumn":15},{"ruleId":"104","