ooks/auth\";\nimport { useToast } from \"../../hooks/toast\";\n// estilo\nimport { Background, Container, Content } from \"./styles\";\n\ninterface SignInFormData {\n  email: string;\n  password: string;\n}\n\nconst SignIn: React.FC = () => {\n  const formRef = useRef<FormHandles>(null);\n\n  const { user, signIn } = useAuth();\n  const { addToast } = useToast();\n\n  const handleSubmit = useCallback(\n    async (data: SignInFormData) => {\n      try {\n        formRef.current?.setErrors({});\n\n        const schema = Yup.object().shape({\n          email: Yup.string()\n            .required(\"Email obrigatório\")\n            .email(\"Digite um e-mail válido\"),\n          password: Yup.string().required(\"Senha obrigatória\"),\n        });\n\n        await schema.validate(data, { abortEarly: false });\n\n        await signIn({ email: data.email, password: data.password });\n      } catch (error) {\n        if (error instanceof Yup.ValidationError) {\n          const errors = getValidationErrors(error);\n\n          formRef.current?.setErrors(errors);\n        }\n\n        addToast({\n          type: \"error\",\n          title: \"Erro na autenticação\",\n          description: \"Ocorreu um erro ao fazer login, cheque as credenciais.\",\n        });\n      }\n    },\n    [signIn, addToast]\n  );\n\n  return (\n    <Container>\n      <Content>\n        <img src={logoImg} alt=\"\" />\n        <Form ref={formRef} onSubmit={handleSubmit}>\n          <h1>Faça seu login</h1>\n          <Input name=\"email\" type=\"text\" placeholder=\"E-mail\" icon={FiMail} />\n          <Input\n            name=\"password\"\n            type=\"password\"\n            placeholder=\"Senha\"\n            icon={FiLock}\n          />\n          <Button type=\"submit\">Entrar</Button>\n          <a href=\"bla\"> Esqueci minha senha</a>\n        </Form>\n        <Link to=\"/signup\">\n          <FiLogIn /> Criar conta\n        </Link>\n      </Content>\n\n      <Background />\n    </Container>\n  );\n};\n\nexport default SignIn;\n","/home/bfcav/revisao-gostack/gobarber/web/src/pages/SignIn/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/Button/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/Button/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/Button/Input/index.tsx",["108"],"import React, {\n  InputHTMLAttributes,\n  useCallback,\n  useEffect,\n  useRef,\n  useState,\n} from \"react\";\nimport { IconBaseProps } from \"react-icons\";\nimport { Container, Error } from \"./styles\";\nimport { useField } from \"@unform/core\";\nimport { FiAlertCircle } from \"react-icons/fi\";\n\ninterface InputProps extends InputHTMLAttributes<HTMLInputElement> {\n  name: string; // tornando nome obrigatório (por default é opcional)\n  icon?: React.ComponentType<IconBaseProps>;\n}\n\nconst Input: React.FC<InputProps> = ({ name, icon: Icon, ...rest }) => {\n  const { fieldName, defaultValue, error, registerField } = useField(name);\n  const inputRef = useRef<HTMLInputElement>(null);\n  //estado para a comunicação do estado focado/não focado do elemento filho pro elemento pai\n  const [isFocused, setIsFocused] = useState(false);\n  const [isFilled, setIsFilled] = useState(false);\n\n  const handleInputFocus = useCallback(() => {\n    setIsFocused(true);\n  }, []);\n\n  const handleInputBlur = useCallback(() => {\n    setIsFocused(false);\n    setIsFilled(!!inputRef.current?.value);\n  }, []);\n\n  useEffect(() => {\n    registerField({\n      name: fieldName,\n      ref: inputRef.current,\n      path: \"value\",\n    });\n  }, [fieldName, registerField]);\n\n  return (\n    <Container isFocused={isFocused} isFilled={isFilled} hasError={!!error}>\n      {Icon && <Icon size={20} />}\n      <input\n        onFocus={handleInputFocus}\n        onBlur={handleInputBlur}\n        ref={inputRef}\n        {...rest}\n      />\n      {error && (\n        <Error title={error} >\n          <FiAlertCircle size={20} color=\"#c53030\" />\n        </Error>\n      )}\n    </Container>\n  );\n};\n\nexport default Input;\n","/home/bfcav/revisao-gostack/gobarber/web/src/components/Button/Input/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/pages/SignUp/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/pages/SignUp/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/utils/getValidationErrors.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/Tooltip/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/Tooltip/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/services/api.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/ToastContainer/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/ToastContainer/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/hooks/auth.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/hooks/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/hooks/toast.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/ToastContainer/Toast/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/ToastContainer/Toast/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/routes/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/pages/Dashboard/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/routes/Route.tsx",[],{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","severity":1,"message":"114","line":27,"column":11,"nodeType":"115","messageId":"116","endLine":27,"endColumn":15},{"ruleId":"113","severity":1,"message":"117","line":19,"column":22,"nodeType":"115","messageId":"116","endLine":19,"endColumn":34},"no-native-reassign",["118"],"no-negated-in-lhs",["119"],"@typescript-eslint/no-unused-vars","'user' is assigned a value but never used.","Identifier","unusedVar","'defaultValue' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]