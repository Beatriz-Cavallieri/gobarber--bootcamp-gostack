ed={isFocused} isFilled={isFilled} hasError={!!error}>\n      {Icon && <Icon size={20} />}\n      <input\n        onFocus={handleInputFocus}\n        onBlur={handleInputBlur}\n        ref={inputRef}\n        {...rest}\n      />\n      {error && (\n        <Error title={error} >\n          <FiAlertCircle size={20} color=\"#c53030\" />\n        </Error>\n      )}\n    </Container>\n  );\n};\n\nexport default Input;\n","/home/bfcav/revisao-gostack/gobarber/web/src/components/Button/Input/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/pages/SignUp/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/pages/SignUp/styles.ts",[],["98","99"],"/home/bfcav/revisao-gostack/gobarber/web/src/utils/getValidationErrors.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/Tooltip/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/Tooltip/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/services/api.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/ToastContainer/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/ToastContainer/styles.ts",[],"/home/bfcav/revisao-gostack/gobarber/web/src/hooks/auth.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/hooks/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/hooks/toast.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/ToastContainer/Toast/index.tsx",[],"/home/bfcav/revisao-gostack/gobarber/web/src/components/ToastContainer/Toast/styles.ts",[],{"ruleId":"100","severity":1,"message":"101","line":4,"column":8,"nodeType":"102","messageId":"103","endLine":4,"endColumn":14},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","replacedBy":"107"},{"ruleId":"100","severity":1,"message":"108","line":23,"column":11,"nodeType":"102","messageId":"103","endLine":23,"endColumn":15},{"ruleId":"100","severity":1,"message":"109","line":19,"column":22,"nodeType":"102","messageId":"103","endLine":19,"endColumn":34},{"ruleId":"104","rep